<Application x:Class="IsaacRandomizer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:IsaacRandomizer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="IsaacHandwritten">pack://application:,,,/#IsaacHandwritten</FontFamily>
        <FontFamily x:Key="AltFont">pack://application:,,,/#AmaticSC-Bold</FontFamily>
    </Application.Resources>
</Application>
